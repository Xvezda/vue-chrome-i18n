  "devDependencies": {
    "@babel/core": "^7.6.4",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "cross-env": "^6.0.3",
    "standard": "^14.3.1",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "resolutions": {
    "**/serialize-javascript": ">=2.1.1",
    "**/minimist": ">=1.2.2"
  }
